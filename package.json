{
  "name": "calm-space",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "clean-empty": "node scripts/clean-folders.js && powershell -Command \"Get-ChildItem .\\app -Recurse | Where-Object { $_.Length -eq 0 } | Remove-Item -Force; Write-Host 'Empty files cleaned!'\"",
    "clean-folders": "node scripts/clean-folders.js",
    "start": "npm run clean-folders && expo start",
    "start:with-ai": "npm run clean-empty && concurrently --kill-others \"node start-ai-simple.js\" \"expo start\"",
    "start:with-enhanced-ai": "npm run clean-empty && concurrently --kill-others \"node start-ai-enhanced.js\" \"expo start\"",
    "start:expo": "npm run clean-empty && expo start",
    "start:ai": "node start-ai-simple.js",
    "start:ai-enhanced": "node start-ai-enhanced.js",
    "start:ai-only": "cd chatbot && python main.py",
    "start:ai-enhanced-only": "cd chatbot && python main_enhanced.py",
    "start:production": "cd chatbot && python server_manager.py",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "npm run clean-empty && npm run start:with-enhanced-ai",
    "production": "npm run start:production",
    "build": "expo build",
    "publish": "expo publish",
    "lint": "expo lint",
    "git:quick": "node scripts/git-quick-update.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/slider": "4.5.6",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "@supabase/supabase-js": "^2.50.2",
    "@tanstack/react-query": "^5.90.2",
    "expo": "~53.0.23",
    "expo-audio": "~0.4.9",
    "expo-av": "~15.1.7",
    "expo-constants": "~17.1.6",
    "expo-document-picker": "~13.1.6",
    "expo-font": "~13.3.1",
    "expo-image": "^2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.7",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-video": "~2.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.3",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-chart-kit"
        ]
      }
    }
  },
  "private": true
}
