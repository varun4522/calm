{
  "name": "calm-space",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:with-ai": "concurrently --kill-others \"node start-ai-simple.js\" \"expo start\"",
    "start:with-enhanced-ai": "concurrently --kill-others \"node start-ai-enhanced.js\" \"expo start\"",
    "start:expo": "expo start",
    "start:ai": "node start-ai-simple.js",
    "start:ai-enhanced": "node start-ai-enhanced.js",
    "start:ai-only": "cd chatbot && python main.py",
    "start:ai-enhanced-only": "cd chatbot && python main_enhanced.py",
    "start:production": "cd chatbot && python server_manager.py",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "npm run start:with-enhanced-ai",
    "production": "npm run start:production",
    "build": "expo build",
    "publish": "expo publish",
    "lint": "expo lint",
    "git:quick": "node scripts/git-quick-update.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@openspacelabs/react-native-zoomable-view": "^2.4.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@react-native-picker/picker": "2.11.1",
    "@shopify/react-native-skia": "2.2.12",
    "@supabase/supabase-js": "^2.50.2",
    "@tanstack/react-query": "^5.90.2",
    "base64-js": "^1.5.1",
    "expo": "~54.0.0",
    "expo-audio": "~1.0.15",
    "expo-av": "~16.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.19",
    "expo-font": "~14.0.9",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-location": "~19.0.7",
